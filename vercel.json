{
  "version": 2,
  "builds": [
    {
      "src": "frontend/frontendapp/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist",
        "installCommand": "cd frontend/frontendapp && npm install",
        "buildCommand": "cd frontend/frontendapp && npm run build"
      }
    },
    {
      "src": "backend/backend-app/api/[...path].ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "backend/backend-app/api/[...path].ts"
    },
    {
      "src": "/(.*\\.(js|css|ico|png|jpg|jpeg|svg|woff|woff2|ttf|eot|json|webp))",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      },
      "dest": "frontend/frontendapp/dist/$1"
    },
    {
      "src": "/(.*)",
      "dest": "frontend/frontendapp/dist/index.html"
    }
  ]
}
