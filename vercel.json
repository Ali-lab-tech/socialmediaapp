{
  "version": 2,
  "builds": [
    {
      "src": "frontend/frontendapp/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist",
        "installCommand": "cd frontend/frontendapp && npm install --production=false",
        "buildCommand": "cd frontend/frontendapp && npm run vercel-build"
      }
    },
    {
      "src": "backend/backend-app/api/[...path].ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "backend/backend-app/api/[...path].ts"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "frontend/frontendapp/dist/$1"
    }
  ]
}
