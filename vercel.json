{
    "version": 2,
    "builds": [
      {
        "src": "frontend/frontendapp/package.json",
        "use": "@vercel/static-build",
        "config": { "distDir": "frontend/frontendapp/dist" }
      },
      {
        "src": "backend/backend-app/package.json",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "backend/backend-app/src/$1"
      },
      {
        "src": "/(.*)",
        "dest": "frontend/frontendapp/dist/$1"
      }
    ]
  }
  