{
  "version": 2,
  "builds": [
    {
      "src": "frontend/frontendapp/package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "dist" }
    },
    {
      "src": "backend/backend-app/dist/main.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "backend/backend-app/dist/main.js"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "frontend/frontendapp/dist/index.html"
    }
  ]
}
