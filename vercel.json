{
  "version": 2,
  "buildCommand": "cd frontend/frontendapp && npm install && npm run build",
  "outputDirectory": "frontend/frontendapp/dist",
  "builds": [
    {
      "src": "backend/backend-app/api/[...path].ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "backend/backend-app/api/[...path].ts"
    },
    {
      "src": "/(.*\\.(js|css|ico|png|jpg|jpeg|svg|woff|woff2|ttf|eot))",
      "dest": "frontend/frontendapp/dist/$1"
    },
    {
      "src": "/(.*)",
      "dest": "frontend/frontendapp/dist/index.html"
    }
  ]
}
